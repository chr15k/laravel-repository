{
    "name": "chr15k/laravel-repository",
    "description": "Repository Layer design pattern for Laravel",
    "keywords": [
        "laravel",
        "chr15k",
        "repository",
        "artisan",
        "php",
        "cli"
    ],
    "homepage": "https://github.com/chr15k/laravel-repository",
    "license": "MIT",
    "authors": [
        {
            "name": "Christopher Keller",
            "email": "christhecoder85@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0",
        "illuminate/console": "^5.0|^6.0|^7.0|^8.0",
        "illuminate/database": "^5.0|^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Chr15k\\Repository\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chr15k\\Repository\\RepositoryServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
